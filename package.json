{
  "name": "functional-test-helper",
  "version": "1.0.0",
  "author": "",
  "description": "Config driven cucumberjs webdriver functional testing helper. ",
  "scripts": {
    "start": "webpack-dev-server",
    "test:development": "npm run lint && npm run test:jest && npm run flow && babel-node scripts/postRunTest.js",
    "test:production": "npm run lint && npm run test:jest && npm run flow:check",
    "flow": "flowcheck",
    "flow:check": "flowcheck check --skip-check",
    "test:jest": "npm run jest:coverage && npm run code-coverage && npm run file-coverage",
    "jest": "jest",
    "jest:coverage": "jest --coverage",
    "jest:update": "jest -u",
    "file-coverage": "node scripts/fileTestCoverage.js",
    "code-coverage": "node scripts/codeCoverageChecker.js",
    "lint": "eslint web/app/themes/freeview/assets/views --ext .jsx --ext .js",
    "lint:cucumber": "eslint features/step_definitions/",
    "lint:mockServer": "eslint docker/mockserver",
    "cucumber:bin": "cucumberjs",
    "cucumber": "node scripts/featureTests.js",
    "cucumber:generateSpreadSheet": "node scripts/generateSpreadSheet.js --exec babel-node --presets es2015,stage-2",
    "cucumber:skipCheck": "node scripts/featureTests.js --skip-check",
    "cucumber:mockServer": "node scripts/featureTests.js --skip-check --mockServer",
    "cucumber:mockServerNoSkip": "node scripts/featureTests.js --mockServer",
    "docker:build": "node scripts/docker.js build",
    "docker:restart": "node scripts/docker.js restart",
    "docker:dbDump": "node scripts/docker.js backup",
    "docker:dbUpdate": "node scripts/docker.js updateDb"
  },
  "jest": {
    "collectCoverage": true
  },
  "bin": {
    "functional-test-helper": "./index.jx"
  },
  "license": "UNLICENSED",
  "private": "true",
  "repository": {
    "type": "git",
    "url": ""
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-es6-polyfill": "1.1.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.0.0",
    "babel-plugin-module-resolver": "2.7.1",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "babel-polyfill": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "babel-register": "https://registry.npmjs.org/babel-register/-/babel-register-6.24.1.tgz",
    "babelify": "7.3.0",
    "chai": "^4.0.2",
    "chromedriver": "^2.31.0",
    "command-line-args": "4.0.6",
    "cucumber": "^2.3.1",
    "dotenv": "https://registry.npmjs.org/dotenv/-/dotenv-4.0.0.tgz",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-import-resolver-babel-module": "3.0.0",
    "eslint-import-resolver-webpack": "0.8.1",
    "eslint-plugin-flowtype": "2.34.0",
    "eslint-plugin-import": "2.3.0",
    "eslint-plugin-jest": "20.0.3",
    "exceljs": "^0.5.1",
    "find-in-files": "0.4.0",
    "flow-check": "0.3.0",
    "geckodriver": "1.8.0",
    "glob": "https://registry.npmjs.org/glob/-/glob-5.0.15.tgz",
    "glob-fs": "0.1.6",
    "husky": "0.13.4",
    "iedriver": "3.3.1-rc.1",
    "inquirer": "https://registry.npmjs.org/inquirer/-/inquirer-0.12.0.tgz",
    "jest": "20.0.4",
    "lodash": "https://registry.npmjs.org/lodash/-/lodash-4.17.4.tgz",
    "moment": "https://registry.npmjs.org/moment/-/moment-2.18.1.tgz",
    "multi-commands": "1.1.1",
    "node-env-file": "^0.1.8",
    "node-fetch": "1.7.0",
    "opn": "https://registry.npmjs.org/opn/-/opn-4.0.2.tgz",
    "phantomjs-prebuilt": "2.1.14",
    "selenium-webdriver": "3.4.0",
    "shelljs": "https://registry.npmjs.org/shelljs/-/shelljs-0.7.7.tgz",
    "sinon": "https://registry.npmjs.org/sinon/-/sinon-1.17.7.tgz",
    "style-parser": "^1.1.1",
    "timemachine": "^0.3.0",
    "tinycolor2": "^1.4.1",
    "user-settings": "0.1.5",
    "vinyl-source-stream": "1.1.0",
    "xml2js": "https://registry.npmjs.org/xml2js/-/xml2js-0.4.17.tgz"
  }
}
